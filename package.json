    "@vitejs/plugin-vue": "^4.6.2",
    "@vueuse/core": "^10.11.1",
    "@vueuse/head": "^2.0.0",
    "autoprefixer": "^10.4.20",
    "highlight.js": "^11.10.0",
    "lodash.clonedeep": "^4.5.0",
    "tailwindcss": "^3.4.14",
