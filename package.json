  "files": [
    "lib",
    "volar.d.ts"
  ],
  "main": "./lib/auth-ui-vue.umd.cjs",
  "module": "./lib/auth-ui-vue.js",
  "exports": {
    ".": {
      "types": "./lib/packages/index.d.ts",
      "import": "./lib/auth-ui-vue.js",
      "require": "./lib/auth-ui-vue.umd.cjs"
    }
  },
  "license": "MIT",
  "keywords": [
    "Supabase",
    "Nuxt",
    "Vue",
    "Auth"
